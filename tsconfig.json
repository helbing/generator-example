{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "declaration": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true
  }
}
